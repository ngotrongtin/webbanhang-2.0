<% unless user.main_admin? %> 
        <%= button_to "delete", user_path(user), method: :delete, data: { confirm: "comfirm it" }, class: "btn btn-danger" %>
        <% if user.admin? %>
            <%= button_to "Unadmin", admin_path(user), method: :post, class: "btn btn-danger" %>
        <% else %>
            <%= button_to "Admin", admin_path(user), method: :post, class: "btn btn-danger" %>
        <% end %>
<% end %>